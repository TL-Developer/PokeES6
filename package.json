  "bugs": {
    "url": "https://github.com/TL-Developer/PokeES6/issues"
  },
  "homepage": "https://github.com/TL-Developer/PokeES6#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "express": "^4.16.3"
  }
