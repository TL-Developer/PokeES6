{
  "name": "pokees6",
  "version": "0.0.1",
  "description": "Teste Tiago Lima para XPI",
  "main": "index.js",
  "scripts": {
    "test": "standard && nodemon ./server.js --exec babel-node -e js",
    "build-css": "node-sass --include-path scss src/sass/style.scss src/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "dev": "nodemon ./server.js --exec babel-node -e js",
    "build": "babel --preset es2015 src -d dist"
  },
